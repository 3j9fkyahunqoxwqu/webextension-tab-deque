{
    "applications": {
        "gecko": {
            "id": "tabdeque@sblask",
            "strict_min_version": "51.0"
        }
    },
    "author": "Sebastian Blask",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "commands": {
        "open-new-tab-at-default-location": {
            "description": "Open new tab at default location",
            "suggested_key": {
                "default": "Ctrl+Y"
            }
        }
    },
    "description": "A firefox extension for better tab handling. Inspired by Opera 12.",
    "homepage_url": "https://addons.mozilla.org/firefox/addon/tab-deque/",
    "icons": {
        "48": "icon.png"
    },
    "manifest_version": 2,
    "name": "Tab Deque",
    "permissions": [
        "contextMenus"
    ],
    "version": "2.0.0b"
}
