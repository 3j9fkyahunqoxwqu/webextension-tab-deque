{
    "applications": {
        "gecko": {
            "id": "tabdeque@sblask",
            "strict_min_version": "57.0"
        }
    },
    "author": "Sebastian Blask",
    "background": {
        "scripts": [
            "browser-polyfill.js",
            "background.js"
        ]
    },
    "commands": {
        "send-tab-to-end-of-tabdeque": {
            "description": "Send tab to end of TabDeque",
            "suggested_key": {
                "default": "Ctrl+Down"
            }
        },
        "select-tab-from-end-of-tabdeque": {
            "description": "Select tab from end of TabDeque",
            "suggested_key": {
                "default": "Ctrl+Up"
            }
        }
    },
    "description": "A firefox extension for better tab handling. Inspired by Opera 12.",
    "homepage_url": "https://addons.mozilla.org/firefox/addon/tab-deque/",
    "icons": {
        "48": "icon.png"
    },
    "manifest_version": 2,
    "name": "Tab Deque",
    "permissions": [
        "contextMenus"
    ],
    "version": "2.0.0"
}
